<section>
    <div class="text-secondary pt-4 pb-5 px-5 hero">
        <div class="container">
          <h1 class="fw-bold text-white mb-4">Explore</h1>
          <p class="fs-5 text-white">
              Search through ALL your previous blogs!
          </p>
        </div>
      </div>
    <div>
</section>
<section>
    <div class="articles container pt-5">
      <h1 class="fw-bold pb-3">All posts</h1>
      <ul class="article-ul">
  
        <% data.forEach(post => { %>
          
          <li>
            <a href="/post/<%= post._id %>">
              <span><%= post.title %></span>
              <span class="article-date"><%= post.createdAt.toDateString() %></span>
            </a>
          </li>
  
        <% }); %>
      </ul>
    </div>
</section>
    